<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="MyTestSuite" thread-count="5" parallel="tests"
	verbose="4">

	<listeners>
		<listener
			class-name="com.qa.AutoloadAI.listeners.ExtentReportListener" />

	</listeners>

 <test name="Autoload AI Home Page Test_edge">
		<parameter name="browser" value="chrome" /> <!-- Define your desired browser here -->
		<classes>
			<class name="com.qa.AutoloadAI.tests.HomePageTest" />
		</classes>
	</test>

	<test name="Autoload AI Login Page Test_edge">
		<parameter name="browser" value="chrome" /> <!-- Define your desired browser here -->
		<classes>
			<class name="com.qa.AutoloadAI.tests.LoginPageTest" />
		</classes>
	</test>



	<test name="Autoload AI ScriptExecute Page Test_edge">
		<parameter name="browser" value="chrome" /> <!-- Define your desired browser here -->
		<classes>
			<class name="com.qa.AutoloadAI.tests.ExecuteScriptTest" />
		</classes>
	</test>


	<test name="Autoload AI Report Page Test_edge">
		<parameter name="browser" value="chrome" /> <!-- Define your desired browser here -->
		<classes>
			<class name="com.qa.AutoloadAI.tests.ReportPageTest" />
		</classes>
	</test>



	<test name="Autoload AI Delete Script Test_edge">
		<parameter name="browser" value="chrome" /> <!-- Define your desired browser here -->
		<classes>
			<class name="com.qa.AutoloadAI.tests.DeleteScriptTest" />
		</classes>
	</test>

 
	<test name="Autoload AI Load Generator Test_edge">
		<parameter name="browser" value="chrome" /> <!-- Define your desired browser here -->
		<classes>
			<class name="com.qa.AutoloadAI.tests.LoadGeneratorTest" />
		</classes>
	</test>



	<test name="Autoload AI Release Management Test_edge">
		<parameter name="browser" value="chrome" /> <!-- Define your desired browser here -->
		<classes>
			<class name="com.qa.AutoloadAI.tests.ReleaseManagementTest" />
		</classes>
	</test>




	<test name="Autoload AI Upload Summary Report Test_edge">
		<parameter name="browser" value="chrome" /> <!-- Define your desired browser here -->
		<classes>
			<class name="com.qa.AutoloadAI.tests.UploadResultTest" />
		</classes>
	</test>


</suite>

