<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="MyTestSuite" thread-count="5" parallel="tests"
	verbose="4">

	<listeners>
		<listener
			class-name="com.qa.AutoloadAI.listeners.ExtentReportListener" />

	</listeners>
	
	<test name="Autoload AI Home Page Test_edge">
		<parameter name="browser" value="chrome" /> <!-- Define your desired browser here -->
		<classes>
			<class name="com.qa.AutoloadAI.tests.HomePageTest" />
		</classes>
	</test>

	<test name="Autoload AI Login Page Test_edge">
		<parameter name="browser" value="chrome" /> <!-- Define your desired browser here -->
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test>
	
	<test name="Autoload AI ScriptExecute Page Test_edge">
		<parameter name="browser" value="chrome" /> <!-- Define your desired browser here -->
		<classes>
			<class name="com.qa.AutoloadAI.tests.ExecuteScriptTest" />
		</classes>
	</test>

	<test name="Autoload AI ScriptExecute Page Test_edge">
		<parameter name="browser" value="chrome" /> <!-- Define your desired browser here -->
		<classes>
			<class name="com.qa.AutoloadAI.tests.ReportPageTest" />
		</classes>
	</test>


